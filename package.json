{
    "name": "workflowy-link-preview",
    "version": "1.0.0",
    "private": true,
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.2.0",
        "@typescript-eslint/parser": "^2.2.0",
        "copy-webpack-plugin": "^5.0.0",
        "eslint": "^6.4.0",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^1.7.0",
        "html-webpack-plugin": "^3.2.0",
        "prettier": "^1.18.2",
        "ts-loader": "^5.3.3",
        "typescript": "^3.6.3",
        "webpack": "^4.29.5",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-chrome-extension-reloader": "^1.2.0",
        "webpack-cli": "^3.2.3"
    },
    "scripts": {
        "dev": "npm run clean && webpack --progress --watch --env.reloader",
        "build:dev": "npm run clean && webpack --progress",
        "build:prod": "npm run clean && webpack --progress --env.production",
        "zip": "zip -r workflowy-link-preview.zip ./dist",
        "deploy": "npm run build:prod && npm run zip",
        "analyze": "npm run clean && webpack --progress --env.production --env.analyze",
        "clean": "rm -rf ./dist && rm -f ./workflowy-link-preview.zip",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "dependencies": {
        "@types/node": "^13.5.0",
        "@types/react": "^16.8.4",
        "@types/react-dom": "^16.8.2",
        "@types/react-redux": "^7.0.1",
        "@types/redux-logger": "^3.0.7",
        "@types/styled-components": "^4.1.19",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "react-redux": "^6.0.1",
        "redux": "^4.0.1",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "styled-components": "^4.4.0",
        "webext-redux": "^2.0.1"
    }
}
