{
    "name": "WorkFlowy Link Preview",
    "description": "Link Previewer for https://workflowy.com/",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["*://workflowy.com/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["http://*/*", "https://*/*"],
    "manifest_version": 2
}
