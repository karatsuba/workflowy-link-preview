{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"], // TODO: fix airbnb eslint style
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["react", "@typescript-eslint"],
    "rules": {}
}
